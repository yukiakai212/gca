{
  "name": "gcajs",
  "version": "1.0.0-beta",
  "description": "Generate GitHub contribution art from images. Draw pixel art on your GitHub contribution graph using commits",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "bin": {
    "gca": "./dist/cli.cjs"
  },
  "scripts": {
    "test": "npm run build && npx vitest run --reporter verbose --coverage",
    "format": "prettier --write **/*.{js,ts}",
    "format:check": "prettier --check **/*.{js,ts}",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "build": "npx tsup"
  },
  "files": [
    "dist/*",
    "CHANGELOG.md"
  ],
  "keywords": [
    "github",
    "github-contributions",
    "contribution-graph",
    "github-art",
    "github-contribution-art",
    "commit-art",
    "git-art",
    "pixel-art",
    "ascii-art",
    "image-to-commits",
    "git-commit-history",
    "git-history",
    "developer-tools",
    "cli",
    "npx",
    "open-source"
  ],
  "author": "Yuki_akai",
  "homepage": "https://github.com/yukiakai212/gca",
  "funding": "https://github.com/sponsors/yukiakai212",
  "engines": {
    "node": ">= 16"
  },
  "maintainers": [
    {
      "name": "Yuki_akai",
      "homepage": "https://github.com/yukiakai212"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yukiakai212/gca.git"
  },
  "bugs": {
    "url": "https://github.com/yukiakai212/gca/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.30.1",
    "@types/parse-github-url": "^1.0.3",
    "@vitest/coverage-v8": "^4.0.15",
    "dotenv": "^17.0.1",
    "eslint": "^9.30.0",
    "pkg-sandbox": "^1.0.1",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.35.1",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "github-contribution-api": "^1.0.1",
    "got": "^14.6.6",
    "parse-github-url": "^1.0.3",
    "sharp": "^0.34.5",
    "zod": "^4.3.5"
  }
}
